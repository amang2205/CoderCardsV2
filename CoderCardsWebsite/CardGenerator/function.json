{
  "scriptFile": "..\\bin\\CodermonCards.dll",
  "entryPoint": "CodermonCards.CodermonCardsGenerator.Run",
  "bindings": [
    {
      "name": "image",
      "type": "blobTrigger",
      "direction": "in",
      "path": "%input-container%/{filename}.jpg",
      "connection": "AzureWebJobsStorage"
    },
    {
      "type": "blob",
      "name": "outputBlob",
      "path": "%output-container%/{filename}.jpg",
      "connection": "AzureWebJobsStorage",
      "direction": "out"
    }
  ],
  "disabled": false
}